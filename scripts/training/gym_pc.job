#!/bin/bash

EPOCHTIME=$(date +%s)

python json_mod.py --log_file ../output/$EPOCHTIME/ \
    --algorithm PPO2 \
    --wind_mode uniform \
    --wind_params -8 0 \
    --scenario perching_long \
    --controller sweep_elevator_cont_rate \
    --turbulence light \
    --timesteps 100000 \
    --start_config -40 -5

nohup python gym_learn.py ../output/$EPOCHTIME/sim_params.json &

# py-spy record -o profile -f speedscope -- python gym_learn.py ../output/$EPOCHTIME/sim_params.json
